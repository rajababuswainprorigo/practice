package com.prorigo.trainning;


class add
{
	
	
}


public class class_object 
{

	
	String n;
	int i;
	
	
	public class_object(String name,int id) 
	{
	//	this.name=name;
		//this.id=id;
		
		n=name;
		i=id;
		System.out.println("this is constructor");
		
		System.out.println(name+" "+id);
	}


	public void disp()
	{
		  System.out.println("Hello");
		  System.out.println(n+" "+i);
	}
	
	
	public void disp1(int i)
	{
		  System.out.println("method 1");
		
	}
	
	
	
	public int disp2(int i)
	{
		  System.out.println("method 2");
		
		  return 5;
	}
	
	public int disp3()
	{
		  System.out.println("method 3");
		
		  int z=6;
		  return z ;
	}
	
	
	
	public static void main(String[] args)
	{
		class_object  ob=new class_object ("fgfg",544);
		
		class_object  ob1=new class_object ("ghgh",645);
		
		
		
	//	int a=ob.disp2(10);
		
		System.out.println(ob.disp3());
	}

}
