package Assignment1;

public class PatternProrigo {


	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		String s ="prorigo";
		int n = s.length();
		
     for(int i=0;i<n;i++) 
     {
			for(int j=0;j<=i;j++)
			{
				System.out.print(s.charAt(j));
			}
	    System.out.println();
	
}
	}

}
